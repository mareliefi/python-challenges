#!/home/marli/Projects/Courses/python-challenges/markdown_to_html_and_jade/venv/bin/python2.7
# -*- coding: utf-8 -*-
import re
import sys

from pyjade.convert import convert_file

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(convert_file())
